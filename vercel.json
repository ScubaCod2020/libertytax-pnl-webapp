{
    "version": 2,
    "routes": [
        {
            "src": "/(.)",
            "dest": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "X-Content-Type-Options", "value": "nosniff" }
            ]
        }
    ]
}
