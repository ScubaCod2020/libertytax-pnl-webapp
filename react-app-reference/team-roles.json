      "githubUsernames": ["@codex"],
      "reviewPaths": ["**/*"],
      "testTypes": ["unit", "integration", "e2e", "mobile", "performance", "security"],
      "deploymentStages": ["preview", "staging", "production"],
      "priority": "high",
