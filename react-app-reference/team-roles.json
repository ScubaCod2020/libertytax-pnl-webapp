      "githubUsernames": ["@codex", "ScubaCod2020"],
      "reviewPaths": [".github/**", "*.yml", "*.yaml", "Dockerfile*", "vercel.json"],
      "testTypes": ["integration", "e2e", "performance"],
      "deploymentStages": ["staging", "production"],
      "priority": "high"
    },
    "security": {
      "name": "Security Team",
      "description": "Security reviews, vulnerability management, compliance, threat assessment",
      "expertise": ["security", "vulnerability", "compliance", "threat-assessment", "auditing"],
      "githubUsernames": ["@codex", "ScubaCod2020"],
      "reviewPaths": ["package*.json", "requirements.txt", "*.lock", "src/auth/**"],
      "testTypes": ["security"],
      "deploymentStages": ["preview", "staging", "production"],
      "priority": "medium"
    },
    "ai": {
      "name": "AI Assistant (@codex)",
      "description": "AI-powered code review, automated testing, debugging assistance, documentation generation",
