  "workflow": {
    "preview": {
      "teams": ["qa", "frontend", "backend", "ai"],
      "duration": "16 minutes",
      "parallel": true
    },
    "staging": {
      "teams": ["qa", "frontend", "backend", "devops", "security", "ai"],
      "duration": "50 minutes", 
      "parallel": true
    },
    "production": {
      "teams": ["qa", "devops", "security", "ai"],
      "duration": "36 minutes",
      "parallel": false
    }
