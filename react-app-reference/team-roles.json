{
  "teams": {
    "frontend": {
      "name": "Frontend Team",
      "description": "UI/UX, React components, responsive design, user experience",
      "expertise": ["typescript", "react", "css", "accessibility", "mobile", "responsive"],
      "githubUsernames": ["@codex", "ScubaCod2020"],
      "reviewPaths": ["src/components/**", "src/styles/**", "public/**", "src/assets/**"],
      "testTypes": ["unit", "integration", "e2e", "mobile", "performance"],
      "deploymentStages": ["preview", "staging", "production"],
      "priority": "high"
    },
    "backend": {
      "name": "Backend Team", 
      "description": "Business logic, calculations, data processing, API development",
